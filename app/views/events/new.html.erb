<h1>New Event</h1>
<p>This will be the place to create a new Event</p>

<%= form_for :event, url: events_path do |f| %>
	<p>
		<%= f.label :title %><br>
		<%= f.text_field :title %>
	</p>

	<p>
		<%= f.label :info %><br>
		<%= f.text_area :info %>
	</p>

	<p>
		<%= f.submit %>
	</p>
<%end%>

<%= link_to 'Back', events_path %>